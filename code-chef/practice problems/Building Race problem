#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,a,b,x,y;
	double c,d,e,f;
	cin>>t;
	if(t>=1&&t<=2500)
	{
	    
	while(t>0)
	{
	    cin>>a>>b>>x>>y;
	    if((a&&b)>=1&&(a&&b)<=100)
	    {
	   c=1.0/x;
	   d=1.0/y;
	   e=c*a;
	   f=d*b;
	   if(e>f)
	   cout<<"Chefina"<<endl;
	    else 
	    if(e<f)
	    cout<<"Chef"<<endl;
	    else 
	   cout<<"Both"<<endl;
	    }
	   t--;
	    
	}
	}
	return 0;
}
